<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wenxr.iot.model.Run" table="run" >
        <id name="runId" type="string">
            <column name="run_id" length="50" />
            <generator class="uuid.hex" />
        </id>
        <property name="userCode" type="string">
            <column name="user_code" length="10" />
        </property>
        <property name="equipmentCode" type="string">
            <column name="equipment_code" length="20" />
        </property>
        <property name="shelfId" type="string">
            <column name="shelf_id" length="10" />
        </property>
        <property name="programName" type="string">
            <column name="program_name" length="30" />
        </property>
        <property name="siteLocation" type="string">
            <column name="site_location" length="10" />
        </property>
        <property name="stepTime" type="string">
            <column name="step_time" length="10" />
        </property>
        <property name="totalTime" type="string">
            <column name="total_time" length="10" />
        </property>
    </class>
</hibernate-mapping>
