<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wenxr.iot.model.History" table="history" >
        <id name="historyId" type="string">
            <column name="history_id" length="50" />
            <generator class="uuid.hex" />
        </id>
        <property name="userCode" type="string">
            <column name="user_code" length="10" />
        </property>
        <property name="equipmentCode" type="string">
            <column name="equipment_code" length="20" />
        </property>
        <property name="coverNumber" type="string">
            <column name="cover_number" length="10" />
        </property>
        <property name="dyeNumber" type="string">
            <column name="dye_number" length="10" />
        </property>
        <property name="productionDate" type="string">
            <column name="production_date" length="10" />
        </property>
    </class>
</hibernate-mapping>
